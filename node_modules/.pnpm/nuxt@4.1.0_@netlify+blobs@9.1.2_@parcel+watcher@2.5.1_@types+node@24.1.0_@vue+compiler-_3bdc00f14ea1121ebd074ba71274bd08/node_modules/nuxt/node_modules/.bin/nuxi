#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/nuxt@4.1.0_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.1.0_@vue+compiler-_3bdc00f14ea1121ebd074ba71274bd08/node_modules/nuxt/bin/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/nuxt@4.1.0_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.1.0_@vue+compiler-_3bdc00f14ea1121ebd074ba71274bd08/node_modules/nuxt/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/nuxt@4.1.0_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.1.0_@vue+compiler-_3bdc00f14ea1121ebd074ba71274bd08/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/nuxt@4.1.0_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.1.0_@vue+compiler-_3bdc00f14ea1121ebd074ba71274bd08/node_modules/nuxt/bin/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/nuxt@4.1.0_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.1.0_@vue+compiler-_3bdc00f14ea1121ebd074ba71274bd08/node_modules/nuxt/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/nuxt@4.1.0_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.1.0_@vue+compiler-_3bdc00f14ea1121ebd074ba71274bd08/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../../bin/nuxt.mjs" "$@"
fi
