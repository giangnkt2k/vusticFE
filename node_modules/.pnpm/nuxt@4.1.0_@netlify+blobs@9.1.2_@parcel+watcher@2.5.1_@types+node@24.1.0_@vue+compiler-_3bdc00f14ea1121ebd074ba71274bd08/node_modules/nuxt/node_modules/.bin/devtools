#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.4_@types+node@24.1.0_jiti@2.5.1_lightningcss@1.30.1_terse_841cd3bd7d2ec54229095f007a5a4179/node_modules/@nuxt/devtools/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.4_@types+node@24.1.0_jiti@2.5.1_lightningcss@1.30.1_terse_841cd3bd7d2ec54229095f007a5a4179/node_modules/@nuxt/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.4_@types+node@24.1.0_jiti@2.5.1_lightningcss@1.30.1_terse_841cd3bd7d2ec54229095f007a5a4179/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.4_@types+node@24.1.0_jiti@2.5.1_lightningcss@1.30.1_terse_841cd3bd7d2ec54229095f007a5a4179/node_modules/@nuxt/devtools/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.4_@types+node@24.1.0_jiti@2.5.1_lightningcss@1.30.1_terse_841cd3bd7d2ec54229095f007a5a4179/node_modules/@nuxt/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.4_@types+node@24.1.0_jiti@2.5.1_lightningcss@1.30.1_terse_841cd3bd7d2ec54229095f007a5a4179/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@nuxt+devtools@2.6.3_vite@7.1.4_@types+node@24.1.0_jiti@2.5.1_lightningcss@1.30.1_terse_841cd3bd7d2ec54229095f007a5a4179/node_modules/@nuxt/devtools/cli.mjs" "$@"
else
  exec node  "$basedir/../../../../../@nuxt+devtools@2.6.3_vite@7.1.4_@types+node@24.1.0_jiti@2.5.1_lightningcss@1.30.1_terse_841cd3bd7d2ec54229095f007a5a4179/node_modules/@nuxt/devtools/cli.mjs" "$@"
