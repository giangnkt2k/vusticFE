#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/@eslint+config-inspector@1.1.0_eslint@9.31.0_jiti@2.4.2_/node_modules/@eslint/config-inspector/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/@eslint+config-inspector@1.1.0_eslint@9.31.0_jiti@2.4.2_/node_modules/@eslint/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/@eslint+config-inspector@1.1.0_eslint@9.31.0_jiti@2.4.2_/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/@eslint+config-inspector@1.1.0_eslint@9.31.0_jiti@2.4.2_/node_modules/@eslint/config-inspector/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/@eslint+config-inspector@1.1.0_eslint@9.31.0_jiti@2.4.2_/node_modules/@eslint/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/@eslint+config-inspector@1.1.0_eslint@9.31.0_jiti@2.4.2_/node_modules:/Users/truonggiangnguyenkhoa/code/breeze-nuxt/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@eslint+config-inspector@1.1.0_eslint@9.31.0_jiti@2.4.2_/node_modules/@eslint/config-inspector/bin.mjs" "$@"
else
  exec node  "$basedir/../../../../../../@eslint+config-inspector@1.1.0_eslint@9.31.0_jiti@2.4.2_/node_modules/@eslint/config-inspector/bin.mjs" "$@"
fi
